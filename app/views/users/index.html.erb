<p id="notice"><%= notice %></p>

<h1>Listing Users</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Avatar</th>
      <th>Last active</th>
      <th>Alumni</th>
      <th>Admin</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.avatar %></td>
        <td><%= user.last_active %></td>
        <td><%= user.alumni %></td>
        <td><%= user.admin %></td>
      </tr>
    <% end %>
  </tbody>
</table>

